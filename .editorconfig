# EditorConfig is awesome: http://EditorConfig.org

# 最上位のEditorConfigファイルにはroot=trueを記述する
root = true

[*]
end_of_line = crlf
insert_final_newline = true
charset = utf-8
indent_style = tab
indent_size = 4

[*.md]
trim_trailing_whitespace = false
indent_style = space
indent_size = 4

[*json]
indent_size = 2

